		<!-- 상품 -->
		<typeAlias alias="product" type="com.icia.bachida.vo.Product"/>
		<!-- 상품옵션 -->
		<typeAlias alias="option" type="com.icia.bachida.vo.Option" />
		<!-- 상품첨부 -->
		<typeAlias alias="attach" type="com.icia.bachida.vo.ProductAttach"/>
		<!-- 상품즐겨찾기 -->
		<typeAlias alias="productChoose" type="com.icia.bachida.vo.ProductBookmark" />
		<!-- 주문옵션 -->
		<typeAlias alias="orderOption" type="com.icia.bachida.vo.OrderOption"/>
		
		<!-- 작가타임라인 -->
		<typeAlias type="com.icia.bachida.vo.ArtisanTimeline" alias="artisanTimeline"/>
		
		<!-- 작가북마크 -->
		<typeAlias type="com.icia.bachida.vo.ArtisanBookmark" alias="bookmark"/>
		
		<!-- 장바구니 옵션 -->
		<typeAlias alias="cartOption" type="com.icia.bachida.vo.CartOption" />
