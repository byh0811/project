		<typeAlias alias="cartProduct" type="com.icia.bachida.vo.CartProduct" />
		<!-- 장바구니 -->
		<typeAlias alias="Cart" type="com.icia.bachida.vo.Cart" />
		<!--  -->
		<typeAlias alias="bid" type="com.icia.bachida.vo.Bid" />
		<typeAlias alias="bidAttach" type="com.icia.bachida.vo.BidAttach" />
		<typeAlias alias="custom" type="com.icia.bachida.vo.Custom" />
		<typeAlias alias="customOpinion" type="com.icia.bachida.vo.CustomOpinion" />
		<typeAlias alias="productionOrder" type="com.icia.bachida.vo.ProductionOrder" />
		
		<!-- 1:1커스텀 -->
		<typeAlias alias="pcustom" type="com.icia.bachida.vo.Pcustom"/>
		<!-- 1:1커스텀 의견조율 -->
		<typeAlias alias="pcustomOpinion" type="com.icia.bachida.vo.PcustomOpinion"/>
		
		<!-- 메인카테고리 -->
		<typeAlias alias="mainCategory" type="com.icia.bachida.vo.MainCategory"/>
		<!-- 메타카테고리 -->
		<typeAlias alias="metaCategory" type="com.icia.bachida.vo.MetaCategory"/>
		
