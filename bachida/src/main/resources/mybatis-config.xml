<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org/DTD config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<!-- 꼭 정의할때 먼저 검색해보고 없으면 자신영역 만들어놓고 정의! 무엇에대한 정의인지 주석으로 써놓기 -->
<configuration>
	<typeAliases>
		<!-- 회원 -->
		<typeAlias alias="user" type="com.icia.bachida.vo.User"/>
		<!-- 작가신청 -->
		<typeAlias alias="artisanApply" type="com.icia.bachida.vo.ArtisanApply"/>
		<!-- 신고 -->
		<typeAlias alias="report" type="com.icia.bachida.vo.Report"/>
		<!-- 메세지 -->
		<typeAlias alias="message" type="com.icia.bachida.vo.Message"/>
		<!-- 캐쉬 -->
		<typeAlias alias="cash" type="com.icia.bachida.vo.Cash"/>
		<!-- 주문 -->
		<typeAlias alias="order" type="com.icia.bachida.vo.Order"/>
		<!-- 주문상품+옵션 -->
		<typeAlias alias="orderProductAndOption" type="com.icia.bachida.dto.OrderProductAndOption"/>
		<!-- 주문상품 -->
		<typeAlias alias="orderProduct" type="com.icia.bachida.vo.OrderProduct"/>
		<!-- 상품평 -->
		<typeAlias alias="comment" type="com.icia.bachida.vo.ProductComment"/>
